{
  "bin": {
    "{{name}}": "src/{{name}}-cli.mjs"
  },
  "keywords": [
    "kronos-integration",
    "systemd"
  ],
  "scripts": {},
  "dependencies": {},
  "optionalDependencies": {
    "@kronos-integration/service-systemd": "^4.0.0",
    "mf-hosting": "^2.9.3"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "cpu": [
    "arm",
    "arm64",
    "x64"
  ],
  "pkgbuild": {
    "content": {
      "${install.dir}": {
        "type": "npm-pack"
      },
      "${install.dir}/node_modules": {
        "type": "node-modules"
      },
      "${systemd.unit.dir}${name}.service": "pkg/${name}.service",
      "${config.dir}config.json": "config/config.json"
    },
    "kronos": true
  },
  "template": {
    "properties": {
      "license": {
        "owner": "darlenya & arlac77"
      },
      "node": {
        "version": "24.10.0"
      }
    },
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-root",
      "arlac77/template-ava",
      "arlac77/template-npm-pkgbuild",
      "arlac77/template-nginx",
      "arlac77/template-systemd",
      "arlac77/template-javascript"
    ],
    "usedBy": [
      "Kronos-Integration/cluster-node",
      "arlac77/component-manager",
      "arlac77/csp-record",
      "arlac77/entitlement-provider",
      "arlac77/hook-ci",
      "arlac77/rtsp-archive",
      "arlac77/system-dashboard",
      "template-tools/template-sync-hook"
    ]
  },
  "packageManager": "npm@11.6.3+sha512.4085a763162e0e3acd19a4e9d23ad3aa0978e501ccf947dd7233c12a689ae0bb0190763c4ef12366990056b34eec438903ffed38fde4fbd722a17c2a7407ee92"
}
